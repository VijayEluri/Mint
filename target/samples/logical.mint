c = a <= b; print(a + ' <= ' + b + ' is ' + c);
c = a == b; print(a + ' == ' + b + ' is ' + c);
c = a != b; print(a + ' != ' + b + ' is ' + c);

print('\nLogical Operator in BigNumbers' );
a = fact(100); b = fact(1000);

